./mvnw clean package -DskipTests

#buildar imagem por meio do dockerfile
docker build --tag nawszera/gamelistapi . (para buildar do docker file)

#mudar a tag
docker tag nawszera/gamelistapi nawszera/gamelistap

#push docker hub
docker push  nawszera/gamelistap (nome da tag do lado)

#comando para container no aws linux
sudo docker run --name nome do container -e APP_PROFILE=prod -e DB_URL=NOME DO SEU DB -e DB_USERNAME=postgres -e DB_PASSWORD=postgres -p 8080:8080 -p 9090:9090 nawszera/gamelistapi

